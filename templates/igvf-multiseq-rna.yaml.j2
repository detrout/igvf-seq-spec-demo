!Assay
seqspec_version: 0.3.0
assay_id: 10x-ATAC-RNA-MULTI
name: 10x-ATAC-RNA-MULTI/Illumina
doi: https://doi.org/10.1038/s41592-019-0433-8
date: 17 June 2019
description: {{ identifier }} Gene expression
modalities:
- rna
lib_struct: https://igvf.github.io/seqspec/
library_protocol: single-nucleus RNA sequencing assay (OBI:0003109)
library_kit: Illumina Truseq Dual Index
sequence_protocol: {{ sequence_protocol }}
sequence_kit: {{ sequence_kit }}
sequence_spec:
- !Read
  read_id: {{ R1_file_id }}
  name: Read 1
  modality: rna
  primer_id: rna-truseq_read1
  min_len: {{ R1_min_len }}
  max_len: {{ R1_max_len }}
  strand: pos
  files:
    - !File
      file_id: {{ R1_file_id }}
      filename: {{ R1_file_name }}
      filetype: {{ R1_file_type }}
      filesize: {{ R1_file_size }}
      url: {{ R1_url }}
      urltype: https
      md5: {{ R1_md5sum }}
- !Read
  read_id: {{ I1_file_id }}
  name: Index 1 (i5 index)
  modality: rna
  primer_id: rna-truseq_read1
  min_len: {{ I1_min_len }}
  max_len: {{ I1_max_len }}
  strand: neg
  files:
    - !File
      file_id: {{ I1_file_id }}
      filename: {{ I1_file_name }}
      filetype: {{ I1_file_type }}
      filesize: {{ I1_file_size }}
      url: {{ I1_url }}
      urltype: https
      md5: {{ I1_md5sum }}
- !Read
  read_id: {{ R2_file_id }}
  name: Read 2
  modality: rna
  primer_id: rna-truseq_read2
  min_len: {{ R2_min_len }}
  max_len: {{ R2_max_len }}
  strand: neg
  files:
    - !File
      file_id: {{ R2_file_id }}
      filename: {{ R2_file_name }}
      filetype: {{ R2_file_type }}
      filesize: {{ R2_file_size }}
      url: {{ R2_url }}
      urltype: https
      md5: {{ R2_md5sum }}
- !Read
  read_id: {{ I2_file_id }}
  name: Index 2 (i7 index)
  modality: rna
  primer_id: rna-truseq_read2
  min_len: {{ I2_min_len }}
  max_len: {{ I2_max_len }}
  strand: pos
  files:
    - !File
      file_id: {{ I2_file_id }}
      filename: {{ I2_file_name }}
      filetype: {{ I2_file_type }}
      filesize: {{ I2_file_size }}
      url: {{ I2_url }}
      urltype: https
      md5: {{ I2_md5sum }}
